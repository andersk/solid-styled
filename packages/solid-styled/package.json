{
  "name": "solid-styled",
  "version": "0.8.2",
  "type": "module",
  "exports": {
    ".": {
      "development": {
        "require": "./dist/cjs/development/index.cjs",
        "import": "./dist/esm/development/index.mjs"
      },
      "require": "./dist/cjs/production/index.cjs",
      "import": "./dist/esm/production/index.mjs",
      "types": "./dist/types/src/index.d.ts"
    },
    "./compiler": {
      "development": {
        "require": "./dist/cjs/development/compiler.cjs",
        "import": "./dist/esm/development/compiler.mjs"
      },
      "require": "./dist/cjs/production/compiler.cjs",
      "import": "./dist/esm/production/compiler.mjs",
      "types": "./dist/types/compiler/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/babel__core": "^7.20.0",
    "@types/babel__traverse": "^7.18.3",
    "@types/node": "^18.15.3",
    "eslint": "^8.39.0",
    "eslint-config-lxsmnsyc": "^0.6.3",
    "pridepack": "^2.4.2",
    "solid-js": "^1.6.15",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "vitest": "^0.30.1"
  },
  "peerDependencies": {
    "solid-js": "^1"
  },
  "dependencies": {
    "@babel/core": "^7.21.3",
    "@babel/helper-module-imports": "^7.18.6",
    "@babel/traverse": "^7.21.3",
    "@babel/types": "^7.21.3",
    "@types/css-tree": "^2.3.1",
    "browserslist": "^4.21.5",
    "css-tree": "^2.3.1",
    "lightningcss": "^1.19.0"
  },
  "description": "Reactive stylesheets for SolidJS",
  "repository": {
    "url": "https://github.com/lxsmnsyc/solid-styled.git",
    "type": "git"
  },
  "homepage": "https://github.com/lxsmnsyc/solid-styled/tree/main/packages/solid-styled",
  "bugs": {
    "url": "https://github.com/lxsmnsyc/solid-styled/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Alexis Munsayac",
  "private": false,
  "scripts": {
    "prepublishOnly": "pridepack clean && pridepack build",
    "build": "pridepack build",
    "type-check": "pridepack check",
    "lint": "pridepack lint",
    "test": "vitest",
    "clean": "pridepack clean",
    "watch": "pridepack watch",
    "start": "pridepack start",
    "dev": "pridepack dev"
  },
  "typesVersions": {
    "*": {
      "compiler": [
        "./dist/types/compiler/index.d.ts"
      ]
    }
  },
  "types": "./dist/types/src/index.d.ts",
  "main": "./dist/cjs/production/index.cjs",
  "module": "./dist/esm/production/index.mjs"
}
